{
  "private": true,
  "name": "@vuetify/loader-dev",
  "version": "0.0.0",
  "scripts": {
    "dev:vite": "vite",
    "build:vite": "NODE_ENV=production vite build",
    "debug:vite": "node --inspect-brk ../node_modules/.bin/vite --debug",
    "debug:vite-build": "NODE_ENV=production node --inspect-brk ../node_modules/.bin/vite build --debug",
    "dev:webpack": "webpack serve --config webpack.config.js --hot",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.js",
    "debug:webpack": "node --inspect-brk ../node_modules/.bin/webpack serve --config webpack.config.js --hot",
    "debug:webpack-build": "NODE_ENV=production node --inspect-brk ../node_modules/.bin/webpack --config webpack.config.js"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "pug": "^3.0.2",
    "pug-plain-loader": "^1.1.0",
    "rollup-plugin-visualizer": "^5.10.0",
    "sass-loader": "^13.3.2",
    "url-loader": "^4.1.1",
    "vite-plugin-inspect": "^0.8.1",
    "vite-plugin-vuetify": "2.1.0",
    "vue-style-loader": "^4.1.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-plugin-vuetify": "3.1.0"
  }
}
